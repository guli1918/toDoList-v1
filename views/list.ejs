<%- include("header") -%>
<body>

    <div class="box" id="heading">
        <h1><%= kindOfDay %></h1>
    </div>

    <div class="box">
            <% for(var i = 0; listItemss.length > i; i++) { %>
            <div class="item">
                <input type="checkbox" id="checkbox">
                <p> <%= listItemss[i] %> </p>
            </div>


            <% } %>

            <form action="/" method="POST">
                <input name="listElement" type="text" placeholder="New Item" autocomplete="off">
                <button type="submit" name="list" value= <%= kindOfDay %> >+</button>
            </form>
    </div>
</body>
<%- include("footer") -%>
</html>